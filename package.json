{
  "name": "fast-json-repair",
  "version": "0.1.0",
  "description": "High-performance JSON repair library for Node.js, powered by Rust",
  "main": "wrapper.js",
  "types": "index.d.ts",
  "napi": {
    "name": "fast-json-repair",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "index.js",
    "wrapper.js",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "*.node"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "node benchmarks/benchmark.mjs",
    "universal": "napi universal",
    "version": "napi version"
  },
  "keywords": [
    "json",
    "repair",
    "parser",
    "rust",
    "performance",
    "llm",
    "ai",
    "napi-rs"
  ],
  "author": "dvideby0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvideby0/node-fast-json-repair.git"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/node": "^20.11.0",
    "jsonrepair": "^3.13.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">= 16"
  }
}
